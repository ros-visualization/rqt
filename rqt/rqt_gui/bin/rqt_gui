#!/usr/bin/env python

import os
import sys

try:
    raise ImportError('foo')
    import rospkg
    r = rospkg.RosPack()
    p = r.get_path('rqt_gui')
    sys.path.append(os.path.join(p, 'src'))
except ImportError:
    import roslib
    roslib.load_manifest('rqt_gui')

from rqt_gui.main import Main
main = Main(os.path.abspath(__file__))
sys.exit(main.main())
